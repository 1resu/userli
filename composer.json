{
    "name": "systemli/management",
    "type": "project",
    "autoload": {
        "psr-4": { "": "src/" },
        "files": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "autoload-dev": {
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.6",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "doctrine/orm": "^2.5",
        "egulias/email-validator": "~1.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "ircmaxell/password-compat": "~1.0.3",
        "knplabs/knp-menu-bundle": "~2.0",
        "mopa/bootstrap-bundle": "~3.0",
        "nelmio/security-bundle": "^2.5",
        "robloach/component-installer": "*",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "sonata-project/admin-bundle": "~3.16",
        "sonata-project/doctrine-orm-admin-bundle": "~3.1",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twbs/bootstrap": "~3.3.0",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "behat/mink-extension": "dev-master",
        "behat/mink-goutte-driver": "*",
        "behat/symfony2-extension": "dev-master",
        "liip/functional-test-bundle": "~1.10",
        "phpunit/phpunit": "~4.5",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
            "ComponentInstaller\\Installer::postAutoloadDump"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true,
        "component-dir": "web/components",
        "component-baseurl": "/components"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "prefer-stable": true
}
